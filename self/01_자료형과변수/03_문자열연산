text1 = "#오늘운동완료"
text2 = "#오운완"
text3 = "#12일차"

total_text = text1 + text2 + text3
print(total_text)

total_text = text1 + " " + text2 + " " + text3 #텍스트 1,2,3 사이에 띄어쓰기
print(total_text)

total_text = text1 + "\n" + text2 + "\n" + text3 #텍스트1,2,3 사이에 enter효과
print(total_text)

total_text = text1 + text2 + text3*3 #텍스트3만 3번 반복함
print(total_text)
